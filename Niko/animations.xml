    <name>walkleft</name>
    <start>
      <x>-4</x>
      <y>0</y>
      <interval>75</interval>
    </start>
    <end>
      <x>-4</x>
      <y>0</y>
      <interval>75</interval>
    </end>
    <sequence repeat="20" repeatfrom="0">
      <frame>4</frame>
      <frame>5</frame>
      <frame>6</frame>
      <frame>7</frame>
